node=$fqdn type=USER_END msg=audit($date.get("dd/MM/yy HH:mm:ss.SSS"):$seq) : pid=3160 uid=root auid=$user ses=1 msg='op=PAM:session_close grantors=pam_loginuid,pam_keyinit,pam_limits,pam_unix,pam_permit,pam_mail,pam_elogind,pam_env acct=$user exe=/usr/lib/lxdm/lxdm-session hostname=$fqdn addr=? terminal=tty1 res=success' 
